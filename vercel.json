{
	"name": "strappazzon",
	"public": false,
	"github": {
		"enabled": true,
		"autoJobCancelation": true,
		"silent": true
	},
	"cleanUrls": true,
	"trailingSlash": false,
	"redirects": [
		{ "source": "/view-source", "destination": "https://github.com/Strappazzon/www", "statusCode": 301 },
		{ "source": "/security.txt", "destination": "/.well-known/security.txt", "statusCode": 301 },
		{ "source": "/files(.+)?", "destination": "/static/$1", "statusCode": 301 },
		{ "source": "/keys(.+)?", "destination": "/accounts", "statusCode": 301 },
		{ "source": "/GRW-GHOST-Buster(.+)?", "destination": "https://strappazzon.github.io/GRW-GHOST-Buster/", "statusCode": 301 },
		{ "source": "/newtab(.+)?", "destination": "https://strappazzon.github.io/newtab/", "statusCode": 301 },
		{ "source": "/PD2-SuperBLT-Hasher(.+)?", "destination": "https://strappazzon.github.io/PD2-SuperBLT-Hasher/", "statusCode": 301 },
		{ "source": "/Screendere-Beat(.+)?", "destination": "https://strappazzon.github.io/Screendere-Beat/", "statusCode": 301 },
		{ "source": "/teleme7ry(.+)?", "destination": "https://strappazzon.github.io/teleme7ry/", "statusCode": 301 },
		{ "source": "/tor-exit-page(.+)?", "destination": "https://strappazzon.github.io/tor-exit-page/$1", "statusCode": 301 }
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "X-Frame-Options",
					"value": "DENY"
				},
				{
					"key": "X-XSS-Protection",
					"value": "1; mode=block"
				},
				{
					"key": "X-Content-Type-Options",
					"value": "nosniff"
				},
				{
					"key": "X-UA-Compatible",
					"value": "IE=edge,chrome=1"
				},
				{
					"key": "Content-Security-Policy",
					"value": "default-src 'none'; script-src 'self'; style-src 'self'; img-src 'self'; font-src 'self'; manifest-src 'self'; frame-ancestors 'none';"
				},
				{
					"key": "Permissions-Policy",
					"value": "accelerometer=(),autoplay=(self),camera=(),fullscreen=(self),geolocation=(),gyroscope=(),interest-cohort=(),microphone=(),payment=(),usb=()"
				},
				{
					"key": "Access-Control-Allow-Origin",
					"value": "*"
				},
				{
					"key": "Access-Control-Allow-Methods",
					"value": "GET, HEAD"
				},
				{
					"key": "Referrer-Policy",
					"value": "no-referrer"
				},
				{
					"key": "Strict-Transport-Security",
					"value": "max-age=31536000; includeSubDomains; preload"
				},
				{
					"key": "Accept-Encoding",
					"value": "br"
				},
				{
					"key": "Cache-Control",
					"value": "no-cache"
				},
				{
					"key": "X-Source-Code",
					"value": "https://github.com/Strappazzon/www"
				}
			]
		},
		{
			"source": "/(.*).ico",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public,max-age=365000000,immutable"
				}
			]
		},
		{
			"source": "/(.*).(eot|otf|ttf|ttc|woff|woff2)",
			"headers": [
				{
					"key": "Cache Control",
					"value": "max-age=365000000,public,immutable"
				}
			]
		}
	]
}
